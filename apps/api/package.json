{
  "name": "@nosql-reco/api",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,json}'",
    "test": "jest",
    "test:e2e": "jest --config jest.config.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "seed": "ts-node src/scripts/seed.ts",
    "reco:precompute": "ts-node src/reco/jobs/precompute.ts",
    "openapi:gen": "ts-node src/scripts/generate-openapi.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "argon2": "^0.31.2",
    "dotenv": "^16.4.5",
    "mongoose": "^8.1.1",
    "pino": "^8.17.0",
    "pino-http": "^9.0.0",
    "redis": "^4.6.13",
    "ulid": "^2.3.0",
    "zod": "^3.22.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-ui-express": "^5.1.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6"
  }
}
